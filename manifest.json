{
  "manifest_version": 2,
  "name": "transitus - YouTube Shorts Converter",
  "description": "Convert YouTube Shorts to ordinary YouTube videos",
  "version": "0.1",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "transitus - YouTube Shorts Converter",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
